function sum(x,y)
{
    return x+y;
}
function sum1(sum,z)
{
    return sum+z;
}
var add=sum(5,7);
var third=sum1(add,3);
console.log(third)

function sum(x,y)
{
    return x+y;
}
function sum1(callbackFunc,z)
{
    return callbackFunc+z;
}
var add=sum(5,7);
var third=sum1(add,3);
console.log(third)
